{
  "name": "receiptchecker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' 'tests/**/*.ts' --write",
    "build": "rm -rf build/ && tsc",
    "start": "node ./build/app.js",
    "watch": "nodemon -L",
    "watch-debug": "nodemon --inspect",
    "test": "jest",
    "cover": "jest --coverage",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate --config ./src/typeorm/config/ormconfig.ts -c test -n migration ",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run --config src/typeorm/config",
    "migration:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert --config src/config",
    "test:migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate --config ./src/typeorm/config/testormconfig.ts",
    "docker:debug": "docker-compose --file docker-compose.debug.yml --compatibility up --build",
    "docker:dev": "docker-compose --file docker-compose.dev.yml --compatibility up --build",
    "docker:prod": "docker-compose --file docker-compose.prod.yml --compatibility up --build -d",
    "docker:debug:down": "docker-compose --file docker-compose.debug.yml --compatibility down",
    "docker:dev:down": "docker-compose --file docker-compose.dev.yml --compatibility down",
    "docker:prod:down": "docker-compose --file docker-compose.prod.yml --compatibility down",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -c test -r src/typeorm/config",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "^27.4.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/request-promise": "^4.1.48",
    "@types/validator": "^13.7.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "image-size": "^1.0.0",
    "jest": "^27.4.7",
    "jimp": "^0.16.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "qrcode-reader": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "telegraf": "^4.5.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "validator": "^13.7.0"
  }
}
